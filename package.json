{
  "name": "void2fa-terminal",
  "version": "1.1.1",
  "description": "A CLI tool for managing 2FA authentication accounts.",
  "license": "MIT",
  "keywords": [
    "2fa",
    "cli",
    "tui",
    "authenticator",
    "hotp",
    "totp"
  ],
  "author": "diegoarauj0",
  "type": "module",
  "main": "index.js",
  "bin": {
    "void2fa": "./dist/index.js",
    "void2fa-tui": "./dist/index.js"
  },
  "scripts": {
    "start:tui": "node dist/tui/index.js",
    "start:cli": "node dist/cli/index.js",
    "dev:tui": "tsx --watch src/tui/index.ts",
    "dev:cli": "tsx src/cli/index.ts",
    "build": "tsc && tsc-alias"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegoarauj0/void2fa-terminal.git"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^5.6.2",
    "clipboardy": "^5.0.0",
    "commander": "^14.0.1",
    "figlet": "^1.9.3",
    "joi": "^18.0.1",
    "keytar": "^7.9.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.26",
    "@types/node": "^24.9.1",
    "@types/speakeasy": "^2.0.10",
    "@vercel/ncc": "^0.38.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
